{
  "name": "tailwindui-pocket",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "fastapi-dev": "set PYTHONIOENCODING=utf-8 && pip3 install -r requirements.txt && fastapi run api/index.py --host 127.0.0.1 --port 8000",
    "dev": "concurrently \"npx next dev\" \"npm run fastapi-dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "browserslist": "defaults, not ie <= 11",
  "dependencies": {
    "@headlessui/react": "^2.1.0",
    "@headlessui/tailwindcss": "^0.2.0",
    "@next/third-parties": "^14.2.4",
    "@tailwindcss/forms": "^0.5.3",
    "@types/node": "^20.10.8",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.12",
    "clsx": "^2.1.0",
    "concurrently": "^8.2.2",
    "framer-motion": "^10.15.0",
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "use-debounce": "^10.0.0"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "sharp": "0.33.1"
  }
}
